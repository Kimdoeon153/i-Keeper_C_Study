#include <stdio.h>

int main() {
    char str[101]; 

    scanf("%s", str);


    for (int i = 0; str[i] != '\0'; i++) {
        if (str[i] >= 'a' && str[i] <= 'z') {
     
            printf("%c", str[i] - 'a' + 'A');
        } else if (str[i] >= 'A' && str[i] <= 'Z') {

            printf("%c", str[i] - 'A' + 'a');
        }
    }
    printf("\n");

    return 0;
}
